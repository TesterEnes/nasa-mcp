# NASA API Configuration
NASA_API_KEY=DEMO_KEY

# Rate Limiting (requests per minute/hour)
RATE_LIMIT_RPM=60
RATE_LIMIT_RPH=1000

# Request Configuration
REQUEST_TIMEOUT=30
MAX_RETRIES=3
RETRY_DELAY=1

# Cache Configuration
ENABLE_CACHE=false
CACHE_TTL=300

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=nasa_apis.log

# MCP Server Configuration
MCP_SERVER_NAME=nasa-apis-mcp
